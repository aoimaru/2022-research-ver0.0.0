[
    {
        "type": "DOCKER-FROM",
        "children": [
            {
                "type": "DOCKER-IMAGE-NAME",
                "value": "debian",
                "children": []
            },
            {
                "type": "DOCKER-IMAGE-TAG",
                "value": "jessie",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-RUN",
        "children": [
            {
                "type": "MAYBE-BASH",
                "value": "apt-get update && apt-get install -y     nginx",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-ADD",
        "children": [
            {
                "type": "DOCKER-ADD-TARGET",
                "children": [
                    {
                        "type": "DOCKER-PATH",
                        "value": "/etc/nginx/",
                        "children": []
                    }
                ]
            },
            {
                "type": "DOCKER-ADD-SOURCE",
                "children": [
                    {
                        "type": "DOCKER-PATH",
                        "value": "nginx.conf",
                        "children": []
                    }
                ]
            }
        ]
    },
    {
        "type": "DOCKER-ADD",
        "children": [
            {
                "type": "DOCKER-ADD-TARGET",
                "children": [
                    {
                        "type": "DOCKER-PATH",
                        "value": "/etc/nginx/sites-available/",
                        "children": []
                    }
                ]
            },
            {
                "type": "DOCKER-ADD-SOURCE",
                "children": [
                    {
                        "type": "DOCKER-PATH",
                        "value": "symfony.conf",
                        "children": []
                    }
                ]
            }
        ]
    },
    {
        "type": "DOCKER-RUN",
        "children": [
            {
                "type": "MAYBE-BASH",
                "value": "ln -s /etc/nginx/sites-available/symfony.conf /etc/nginx/sites-enabled/symfony && rm /etc/nginx/sites-enabled/default",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-RUN",
        "children": [
            {
                "type": "MAYBE-BASH",
                "value": "echo \"upstream php-upstream { server php:9000; }\" > /etc/nginx/conf.d/upstream.conf",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-RUN",
        "children": [
            {
                "type": "MAYBE-BASH",
                "value": "usermod -u 1000 www-data",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-CMD",
        "children": [
            {
                "type": "DOCKER-CMD-ARG",
                "value": "nginx",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-EXPOSE",
        "children": [
            {
                "type": "DOCKER-PORT",
                "value": "80",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-EXPOSE",
        "children": [
            {
                "type": "DOCKER-PORT",
                "value": "443",
                "children": []
            }
        ]
    }
]