[
    {
        "type": "DOCKER-FROM",
        "children": [
            {
                "type": "DOCKER-IMAGE-NAME",
                "value": "debian9",
                "children": []
            },
            {
                "type": "DOCKER-IMAGE-REPO",
                "value": "launcher.gcr.io",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-RUN",
        "children": [
            {
                "type": "MAYBE-BASH",
                "value": "apt-get update",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-RUN",
        "children": [
            {
                "type": "MAYBE-BASH",
                "value": "apt-get install -y bash curl wget gnupg apt-transport-https apt-utils lsb-release",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-RUN",
        "children": [
            {
                "type": "MAYBE-BASH",
                "value": "wget -O - https://debian.neo4j.org/neotechnology.gpg.key | apt-key add -",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-RUN",
        "children": [
            {
                "type": "MAYBE-BASH",
                "value": "curl https://packages.cloud.google.com/apt/doc/apt-key.gpg | apt-key add -",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-RUN",
        "children": [
            {
                "type": "MAYBE-BASH",
                "value": "echo 'deb https://debian.neo4j.org/repo stable/' | tee -a /etc/apt/sources.list.d/neo4j.list",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-RUN",
        "children": [
            {
                "type": "MAYBE-BASH",
                "value": "echo \"deb http://packages.cloud.google.com/apt cloud-sdk-$(lsb_release -c -s) main\" | tee -a /etc/apt/sources.list.d/google-cloud-sdk.list",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-RUN",
        "children": [
            {
                "type": "MAYBE-BASH",
                "value": "echo \"neo4j-enterprise neo4j/question select I ACCEPT\" | debconf-set-selections",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-RUN",
        "children": [
            {
                "type": "MAYBE-BASH",
                "value": "echo \"neo4j-enterprise neo4j/license note\" | debconf-set-selections",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-RUN",
        "children": [
            {
                "type": "MAYBE-BASH",
                "value": "apt-get update",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-RUN",
        "children": [
            {
                "type": "MAYBE-BASH",
                "value": "apt-get install -y neo4j-enterprise=1:3.5.3 google-cloud-sdk",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-RUN",
        "children": [
            {
                "type": "MAYBE-BASH",
                "value": "mkdir /data",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-ADD",
        "children": [
            {
                "type": "DOCKER-ADD-TARGET",
                "children": [
                    {
                        "type": "DOCKER-PATH",
                        "value": "/scripts/backup.sh",
                        "children": []
                    }
                ]
            },
            {
                "type": "DOCKER-ADD-SOURCE",
                "children": [
                    {
                        "type": "DOCKER-PATH",
                        "value": "backup/backup.sh",
                        "children": []
                    }
                ]
            }
        ]
    },
    {
        "type": "DOCKER-RUN",
        "children": [
            {
                "type": "MAYBE-BASH",
                "value": "chmod +x /scripts/backup.sh",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-CMD",
        "children": [
            {
                "type": "DOCKER-CMD-ARG",
                "value": "/scripts/backup.sh",
                "children": []
            }
        ]
    }
]