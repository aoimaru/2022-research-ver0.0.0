    },
    {
        "type": "DOCKER-RUN",
        "children": [
            {
                "type": "MAYBE-BASH",
                "value": "apt-get update > /dev/null &&     DEBIAN_FRONTEND=noninteractive apt-get install -y --no-install-recommends         curl         docker.io         jq         openssh-server         vim > /dev/null &&     apt-get clean",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-RUN",
        "children": [
            {
