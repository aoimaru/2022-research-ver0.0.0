    {
        "type": "DOCKER-RUN",
        "children": [
            {
                "type": "MAYBE-BASH",
                "value": "apt-get install -y libboost-all-dev libzmq3-dev curl wget",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-RUN",
        "children": [
            {
                "type": "MAYBE-BASH",
                "value": "mkdir ~/source",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-RUN",
        "children": [
            {
                "type": "MAYBE-BASH",
                "value": "cd ~/source && wget https://github.com/namecoin/namecoin-core/archive/nc0.16.3.tar.gz -O nc0.16.3.tar.gz   && tar zxf nc0.16.3.tar.gz",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-RUN",
        "children": [
            {
                "type": "MAYBE-BASH",
                "value": "cd ~/source/namecoin-core-nc0.16.3   && mkdir -p /usr/local/db4   && wget 'http://download.oracle.com/berkeley-db/db-4.8.30.NC.tar.gz'   && tar -xzvf db-4.8.30.NC.tar.gz   && cd db-4.8.30.NC/build_unix   && ../dist/configure --enable-cxx --disable-shared --with-pic --prefix=/usr/local/db4   && make install",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-RUN",
        "children": [
            {
                "type": "MAYBE-BASH",
                "value": "cd ~/source/namecoin-core-nc0.16.3   && ./autogen.sh   && ./configure LDFLAGS=\"-L/usr/local/db4/lib\" CPPFLAGS=\"-I/usr/local/db4/include\" --without-miniupnpc   && make -j2 && make install",
                "children": []
            }
        ]
    },
