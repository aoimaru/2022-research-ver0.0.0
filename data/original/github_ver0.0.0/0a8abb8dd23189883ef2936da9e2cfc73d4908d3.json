                "type": "DOCKER-IMAGE-TAG",
                "value": "8",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-ARG",
        "children": [
            {
                "type": "DOCKER-NAME",
                "value": "SPRING_PROFILES_ACTIVE",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-ARG",
        "children": [
            {
                "type": "DOCKER-NAME",
                "value": "JAVA_OPTS",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-ARG",
        "children": [
            {
                "type": "DOCKER-NAME",
                "value": "PORT",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-ENV",
        "children": [
            {
                "type": "DOCKER-NAME",
                "value": "SPRING_PROFILES_ACTIVE",
                "children": []
            },
            {
                "type": "DOCKER-LITERAL",
                "value": "${SPRING_PROFILES_ACTIVE:-docker}",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-ENV",
        "children": [
            {
                "type": "DOCKER-NAME",
                "value": "JAVA_OPTS",
                "children": []
            },
            {
                "type": "DOCKER-LITERAL",
                "value": "${JAVA_OPTS:-'-Xmx512m'}",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-ENV",
        "children": [
            {
                "type": "DOCKER-NAME",
                "value": "DEBUG_OPTS",
                "children": []
            },
            {
                "type": "DOCKER-LITERAL",
                "value": "${DEBUG_OPTS}",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-ENV",
        "children": [
            {
                "type": "DOCKER-NAME",
                "value": "PORT",
                "children": []
            },
            {
                "type": "DOCKER-LITERAL",
                "value": "${PORT:-8081}",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-ADD",
        "children": [
            {
                "type": "DOCKER-ADD-TARGET",
                "children": [
                    {
                        "type": "DOCKER-PATH",
                        "value": "/app.jar",
                        "children": []
                    }
                ]
            },
            {
                "type": "DOCKER-ADD-SOURCE",
                "children": [
                    {
                        "type": "DOCKER-PATH",
                        "value": "*.jar",
                        "children": []
                    }
                ]
            }
        ]
    },
    {
        "type": "DOCKER-VOLUME",
        "children": [
            {
                "type": "DOCKER-PATH",
                "value": "/tmp",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-RUN",
        "children": [
            {
                "type": "MAYBE-BASH",
                "value": "sh -c 'touch /app.jar'",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-RUN",
        "children": [
            {
                "type": "MAYBE-BASH",
                "value": "apt-get update && apt-get install -y wget",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-RUN",
        "children": [
            {
                "type": "MAYBE-BASH",
                "value": "wget https://github.com/jwilder/dockerize/releases/download/v0.1.0/dockerize-linux-amd64-v0.1.0.tar.gz",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-RUN",
        "children": [
            {
                "type": "MAYBE-BASH",
                "value": "tar -C /usr/local/bin -xzvf dockerize-linux-amd64-v0.1.0.tar.gz",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-EXPOSE",
        "children": [
            {
                "type": "DOCKER-PORT",
                "value": "${PORT}",
                "children": []
            }
        ]
    },
    {
        "type": "DOCKER-CMD",
        "children": [
            {
                "type": "DOCKER-CMD-ARG",
                "value": "dockerize -wait http://config-server:8888/health -timeout 60s java ${JAVA_OPTS} ${DEBUG_OPTS} -Djava.security.egd=file:/dev/./urandom -jar /app.jar",
                "children": []
            }
        ]
    }
]